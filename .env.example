        # API Configuration
        PROJECT_NAME="Bibliotheca Chatbot API"
        DEBUG=true 
        LOG_LEVEL=DEBUG

        # CORS
        CORS_ORIGINS=["*"] 

        # Database Connections
        DATABASE_URLS="report_management=postgresql://DB_USER:DB_PASSWORD@DB_HOST:DB_PORT/report_management,chat_management=postgresql://DB_USER:DB_PASSWORD@DB_HOST:DB_PORT/chat_management"
